{
    "name": "littler/kernel",
    "type": "library",
    "license": "MPL-2.0",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "little hyperf kernel",
    "autoload": {
        "psr-4": {
            "Littler\\Kernel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=8.0",
        "hyperf/cache": "^2.2",
        "hyperf/command": "~2.2.0",
        "hyperf/config": "^2.2",
        "hyperf/config-aliyun-acm": "^2.2",
        "hyperf/config-center": "^2.2",
        "hyperf/config-nacos": "^2.2",
        "hyperf/constants": "~2.2.0",
        "hyperf/crontab": "^2.2",
        "hyperf/database": "~2.2.0",
        "hyperf/db-connection": "~2.2.0",
        "hyperf/devtool": "^2.2",
        "hyperf/di": "^2.2",
        "hyperf/event": "^2.2",
        "hyperf/filesystem": "^2.2",
        "hyperf/flysystem-oss": "^1.0",
        "hyperf/framework": "~2.2.0",
        "hyperf/guzzle": "^2.2",
        "hyperf/http-server": "~2.2.0",
        "hyperf/logger": "^2.2",
        "hyperf/memory": "~2.2.0",
        "hyperf/model-cache": "~2.2.0",
        "hyperf/pool": "^2.2",
        "hyperf/process": "~2.2.0",
        "hyperf/redis": "^2.2",
        "hyperf/resource": "^2.2",
        "hyperf/snowflake": "^2.2",
        "hyperf/tracer": "~2.2.0",
        "hyperf/validation": "^2.2",
        "hyperf/websocket-server": "^2.2",
        "nette/php-generator": "^3.6",
        "nikic/php-parser": "^4.13"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": ">=7.0",
        "firstphp/wsdebug": "^1.1",
        "hyperf/ide-helper": "~2.2.0",
        "hyperf/testing": "~2.2.0",
        "hyperf/watcher": "^2.2",
        "phpstan/phpstan": "^0.12",
        "swoole/ide-helper": "^4.5",
        "swow/swow": "dev-develop"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml --colors=always",
        "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "Littler\\Kernel\\ConfigProvider"
        }
    }
}
